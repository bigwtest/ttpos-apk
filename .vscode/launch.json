  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "pos",
      "cwd": "apps/pos",
      "request": "launch",
      "type": "dart",
      "openDevTools": "network",
      "args": ["--dart-define-from-file=.env.development.local"]
    },
    {
      "name": "assistant",
      "cwd": "apps/assistant",
      "request": "launch",
      "type": "dart",
      "openDevTools": "network",
      "args": ["--dart-define-from-file=.env.development.local"]
    },
    {
      "name": "tablet",
      "cwd": "apps/tablet",
      "request": "launch",
      "type": "dart",
      "openDevTools": "network",
      "args": ["--dart-define-from-file=.env.development.local"]
    },
    {
      "name": "kds",
      "cwd": "apps/kds",
      "request": "launch",
      "type": "dart",
      "openDevTools": "network",
      "args": ["--dart-define-from-file=.env.development.local"]
    },
    {
      "name": "mobile",
      "cwd": "apps/mobile",
      "request": "launch",
      "type": "dart",
      "args": [
        "-d",
        "chrome",
        "--web-port=50900",
        "--dart-define-from-file=.env.development.local"
      ]
    },
    {
      "name": "menu",
      "cwd": "apps/menu",
      "request": "launch",
