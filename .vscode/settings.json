{
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "[xml]": {
    "editor.formatOnSave": false,
    "editor.formatOnPaste": false,
    "editor.formatOnType": false
  },
  "cSpell.words": [
    "arraybuffer",
    "audioplayers",
    "COCOAPODS",
    "CODESOFT",
    "compax",
    "cupertino",
    "dingdingding",
    "dotenv",
    "Encrypter",
    "fenix",
    "Getx",
    "gomobile",
    "gorm",
    "goserver",
    "HOWBEST",
    "Iconfont",
    "libhttpserver",
    "Noto",
    "PODFILE",
    "pointycastle",
    "Railgun",
    "Roboto",
    "shuaxin",
    "sppos",
    "sqflite",
    "subscreen",
    "Sunmi",
    "TTPOS",
    "unfreezed",
    "wakelock",
    "XPRINTER",
    "zhtw",
    "Zhuhai"
  ],
  "files.exclude": {
    // "**/*.freezed.dart": true,
    // "**/*.g.dart": true
  },
  "search.exclude": {
    "**/.next": true,
    "**/*.freezed.dart": true,
    "**/*.g.dart": true,
    "**/dist": true,
    "**/node_modules": false,
    "**/node_modules/**": true,
    "**/vendor": true
  },
  "dart.previewFlutterUiGuides": true,
  "todo-tree.general.statusBar": "total",
  "todo-tree.general.automaticGitRefreshInterval": 60,
  "todo-tree.general.periodicRefreshInterval": 1,
  "todo-tree.general.showActivityBarBadge": true,
  "todo-tree.general.tags": [
    "BUG",
    "NOTE",
    "FIXME",
    "TODO",
    "HACK"
  ],
  "todo-tree.highlights.useColourScheme": true,
  "todo-tree.tree.expanded": true,
  "todo-tree.tree.groupedByTag": true,
  "todo-tree.tree.showCountsInTree": true,
  "dart.lineLength": 120,
  "dart.promptToGetPackages": false,
  "dart.runPubGetOnPubspecChanges": "never",
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/{info,lfs,logs,refs,objects}/**",
    ".git/{index,*refs,*HEAD}",
    ".vscode",
    ".vscode-insiders",
    "packages/i18n/lib/langs/*.dart",
    "scripts/build_mac.sh"
  ]
}